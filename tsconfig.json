{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "."
  },
  "include": [
    "./MeowBlog/.vuepress/**/*.ts",
    "./MeowBlog/.vuepress/**/*.vue"
  ],
  "exclude": [
    "node_modules",
    "dist",
    ".temp",
    ".cache"
  ]
}
